{
    "name": "grafite/auth",
    "description": "Auth backend and Controller stubs for the Laravel Framework from Laravel UI",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/console": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/filesystem": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "^6.23|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Grafite\\Auth\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Grafite\\Auth\\GrafiteAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
